{
  "name": "webgen",
  "version": "1.0.0",
  "description": "An open source personal website generator",
  "main": "./backend/index.js",
  "scripts": {
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --watch --config ./jest.config.js",
    "test:coverage": "jest --coverage --colors --config ./jest.config.js",
    "test:leak": "jest --config ./jest.config.js --runInBand --detectOpenHandles",
    "start": "node backend/index.js",
    "dev": "nodemon backend/index.js",
    "conc": "concurrently 'npm run dev' 'npm run bundle'",
    "prod": "NODE_ENV=production node ./backend/index.js",
    "bundle": "webpack --mode=development --hot --inline --progress --watch --env.type=app",
    "build": "NODE_ENV=production webpack --mode=production --env.type=app",
    "build:user": "NODE_ENV=production webpack --mode=production --env.type=user",
    "prettify": "prettier --config ./.prettierrc --write **/*.{js,jsx,md}",
    "prettify:watch": "prettier --config ./.prettierrc --check **/*.{js,jsx,md}",
    "lint": "eslint --ignore-path .eslintignore --cache --color --no-error-on-unmatched-pattern --fix **/ --ext .js,.jsx",
    "lint:watch": "eslint --ignore-path .eslintignore --cache --color --no-error-on-unmatched-pattern **/ --ext .js,.jsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sid200026/WebGen.git"
  },
  "keywords": [
    "NodeJS",
    "ExpressJS",
    "ReactJS",
    "Personal-Website",
    "Generator"
  ],
  "author": "Siddharth Singha Roy <siddharthsingharoy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sid200026/WebGen/issues"
  },
  "homepage": "https://github.com/Sid200026/WebGen#readme",
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@reach/router": "^1.3.3",
    "archiver": "^5.0.2",
    "aws-sdk": "^2.795.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bull": "^3.18.1",
    "clsx": "^1.1.0",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.4.0",
    "ioredis": "^4.19.4",
    "lodash.throttle": "^4.1.1",
    "mongoose": "^5.9.13",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.16",
    "pg": "^8.5.1",
    "prop-types": "^15.7.2",
    "rate-limit-redis": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-helmet": "^6.1.0",
    "react-material-ui-carousel": "^2.1.2",
    "react-particles-js": "^3.4.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-tsparticles": "^1.18.11",
    "recharts": "^1.8.5",
